<Page x:Class="A_Mapping2.Views.Pages.MindMapPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:A_Mapping2.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MindMapPage">
    <Grid Margin="20">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" >
            <TextBlock x:Name="MapTitle" FontSize="24" FontWeight="Bold" TextAlignment="Center"/>
            <Image x:Name="MapImage" Width="500" Height="300" Stretch="Uniform"/>
        </StackPanel>
        <Button Content="Back" Click="BackButton_Click" Width="100" Height="50"/>

    </Grid>
</Page>
