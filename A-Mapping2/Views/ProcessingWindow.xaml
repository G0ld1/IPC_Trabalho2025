<Window x:Class="A_Mapping2.Views.ProcessingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:A_Mapping2.Views"
        mc:Ignorable="d"
        Title="ProcessingWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize" WindowStyle="None" ShowInTaskbar="False" Topmost="True">
    <Grid Background="White">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="A processar o seu ficheiro..." FontSize="18" Margin="0 0 0 20" HorizontalAlignment="Center"/>
            <ProgressBar x:Name="ProgressBar" Width="250" Height="20" Minimum="0" Maximum="100" Value="0"/>
        </StackPanel>
    </Grid>
</Window>
